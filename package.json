   },
   "devDependencies": {
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/node": "^16.18.25",
      "ts-node": "10.7.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "body-parser": "^1.20.2",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "mysql": "^2.18.1",
      "nodemon": "^2.0.22",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^4.6.2",
      "typeorm": "0.3.15"
   }
}
